{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/[...action]"
    },
    {
      "src": "/api/prestadores/(.*)",
      "dest": "/api/prestadores/[...params]"
    }
  ]
}
